pipeline{
    agent any
    stages{
        stage("CompilePhase"){
            steps{
                bat "cd myfirstrestservice"
                bat "mvn compile"
                }
            }

        stage("testPhase"){
            steps{
                bat "cd myfirstrestservice"
                bat "mvn test"
                }
        }

        stage("packagePhase"){
        steps{
            bat "cd myfirstrestservice"
            bat "mvn clean package"
                }
            }
        }
}
