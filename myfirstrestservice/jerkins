pipeline{
    agent any
    stages{
        stage("CompilePhase"){
            steps{
                bat "cd myfirstrestservice & mvn compile"
                }
            }

        stage("testPhase"){
            steps{
                bat "cd myfirstrestservice & mvn test"
                }
        }

        stage("packagePhase"){
        steps{
            bat "cd myfirstrestservice & mvn clean package"
                }
            }
        }
}
